package finals;
import java.util.*;
public class jags {
	public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int r1= sc.nextInt();
	int c1 = sc.nextInt();
	int [] [] a1 = new int[r1][c1];
	for(int i=0;i<a1.length;i++) {
		for(int j=0;j<a1[0].length;j++) {
			a1[i][j]=sc.nextInt();
		}
	}
	for(int j=0;j<a1[0].length;j++) {
		if(j%2==0) {
	for(int i=0;i<a1.length;i++) {
		System.out.println(a1[i][j]);
	}
		}
		else {
			for(int i=a1.length-1;i>=0;i--) {
		System.out.println(a1[i][j]);
			}
		}
	}
}
}
